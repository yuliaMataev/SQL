<%- include('./header')-%>
<div class="container">
  <table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">First Name</th>
        <th scope="col">Last Name</th>
        <th scope="col">Email</th>
        <th scope="col">Phone</th>
      </tr>
    </thead>
    <tbody>
      <% users.forEach((element,index)=> { %>
      <tr>
        <th scope="row"><%= index+1 %></th>
        <td><%= element.FirstName %></td>
        <td><%= element.LastName %></td>
        <td><%= element.Email %></td>
        <td><%= element.Phone %></td>
      </tr>
      <%   }); %>
    </tbody>

  </table>
</div>